<h1>Listing events</h1>

<table>
  <thead>
    <tr>
      <th>Starts at</th>
      <th>Ends at</th>
      <th>User</th>
      <th>Location</th>
      <th>Title</th>
      <th>Discription</th>
      <th>Catagory</th>
      <th>All day</th>
      <th>Lat</th>
      <th>Long</th>
      <th>Geocoded</th>
      <th>Picture</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.starts_at %></td>
        <td><%= event.ends_at %></td>
        <td><%= event.user_id %></td>
        <td><%= event.location %></td>
        <td><%= event.title %></td>
        <td><%= event.discription %></td>
        <td><%= event.catagory %></td>
        <td><%= event.all_day %></td>
        <td><%= event.lat %></td>
        <td><%= event.long %></td>
        <td><%= event.geocoded %></td>
        <td><%= event.picture %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
