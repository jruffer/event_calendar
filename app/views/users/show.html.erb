<p id="notice"><%= notice %></p>

<p>
  <strong>First name:</strong>
  <%= @user.first_name %>
</p>

<p>
  <strong>Last name:</strong>
  <%= @user.last_name %>
</p>

<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>

<p>
  <strong>Password:</strong>
   ************
</p>

<p>
  <strong>Dob:</strong>
  <%= @user.dob %>
</p>
<ul>
  <%if @user.invites.present?%>
    <% @user.events.each do |event| %>
      <li>
        <b>Title:</b> <%= event.title %><br/>
        <b>Discription:</b> <%= event.discription %><br/>
        <b>Starts at:</b> <%= event.starts_at.strftime("") %><br/>
        <b>Ends at:</b> <%= event.ends_at.strftime("") %><br/>
        <b>Host:</b> <%= event.host %><br/>
      </li>
    <% end %>
  <% end %>
</ul>

<%= link_to 'Edit', edit_user_path(@user) %> |
<%= link_to 'Back', users_path %>
